   "source": [
    "def bayes_theorem(p_f, p_f_and_a):\n",
    "    p_a_given_f = (p_f_and_a) / p_f \n",
    "    return p_a_given_f\n",
    "# P(F) \n",
    "p_f = 0.2\n",
    "# P(F^A)\n",
    "p_f_and_a = 0.03\n",
    "# calculate P(A|F)\n",
    "result = bayes_theorem(p_f, p_f_and_a)\n",
    "# summarize\n",
    "print('P(A|F) = %.3f%%' % (result * 100))"
   ]
